@model IEnumerable<DogWalk.Data.Models.WalkModel>

@{
    ViewBag.Title = "GetListOfScheduledWalks";
}

<h2>GetListOfScheduledWalks</h2>

<p>
    @Html.ActionLink("Create New", "ScheduleWalksByDate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DateOfWalk)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfWalk)
        </td>
        <td>
            @Html.ActionLink("Details", "GetWalkByID", new { id=item.ID }) |
            @Html.ActionLink("Cancel", "CancelWalk", new { id=item.ID })
        </td>
    </tr>
}

</table>
